{
  "name": "bundesdenkmal-checker",
  "version": "0.0.0",
  "description": "Vergleiche die Liste der Baudenkmäler des Österreichischen Bundesdenkmalamtes mit Wikidata, Wikipedia, Wikimedia Commons und OpenStreetMap.",
  "main": "index.js",
  "scripts": {
    "build": "mkdir -p dist && browserify src/app.js -o dist/app.js",
    "watch": "mkdir -p dist && watchify --debug src/app.js -o dist/app.js -v",
    "update": "mkdir -p data && node update.js",
    "lint": "standard --fix update.js server.js src/*.js server/*.js checks/*.js",
    "prepare": "npm run build",
    "start": "node server.js",
    "test": "mocha --bail"
  },
  "repository": "https://github.com/plepe/bundesdenkmal-checker",
  "author": "Stephan Bösch-Plepelits <skunk@xover.mud.at>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plepe/bundesdenkmal-checker"
  },
  "homepage": "https://github.com/plepe/bundesdenkmal-checker",
  "dependencies": {
    "async": "^3.2.0",
    "csvtojson": "^2.0.10",
    "foreach": "^2.0.5",
    "html-escape": "^2.0.0",
    "iconv-lite": "^0.6.0",
    "jsdom": "^16.2.2",
    "natsort": "^2.0.2",
    "node-fetch": "^2.6.0",
    "query-string": "^6.13.1",
    "sheet-router": "^4.2.3",
    "w3c-xmlhttprequest": "^2.1.3"
  },
  "devDependencies": {
    "browserify": "^16.5.1",
    "mocha": "^8.0.1",
    "standard": "^14.3.4",
    "watchify": "^3.11.1"
  }
}
