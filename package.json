{
  "name": "osm-wikidata-bda",
  "version": "0.0.0",
  "description": "Vergleiche die Liste der Baudenkmäler des Bundesdenkmalamtes mit OpenStreetMap und Wikidata.",
  "main": "index.js",
  "scripts": {
    "build": "mkdir -p dist && browserify src/app.js -o dist/app.js",
    "watch": "mkdir -p dist && watchify --debug src/app.js -o dist/app.js -v",
    "update": "mkdir -p data && node update.js",
    "lint": "standard --fix update.js src/*.js",
    "prepare": "npm run build && npm run update",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plepe/osm-wikidata-bda.git"
  },
  "author": "Stephan Bösch-Plepelits <skunk@xover.mud.at>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plepe/osm-wikidata-bda/issues"
  },
  "homepage": "https://github.com/plepe/osm-wikidata-bda#readme",
  "dependencies": {
    "async": "^3.2.0",
    "csvtojson": "^2.0.10",
    "html-escape": "^2.0.0",
    "iconv-lite": "^0.6.0",
    "natsort": "^2.0.2",
    "node-fetch": "^2.6.0",
    "query-string": "^6.13.1",
    "sheet-router": "^4.2.3",
    "w3c-xmlhttprequest": "^2.1.3"
  },
  "devDependencies": {
    "browserify": "^16.5.1",
    "standard": "^14.3.4",
    "watchify": "^3.11.1"
  }
}
